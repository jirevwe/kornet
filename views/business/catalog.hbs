 <body>
    <!-- Page Content -->
    <div class="container">

        <!-- Jumbotron Header -->
        <header class="jumbotron hero-spacer">
            <h1>{{ catalog.name }}</h1>
            <p>{{{catalog.content}}}</p>
            <p><a class="btn btn-primary btn-large" href="/business/edit/{{catalog._id}}">Edit</a>
            </p>
        </header>
        <hr>
        <!-- Title -->
        <div class="row">
            <div class="col-lg-12">
                <h3>Products</h3>
            </div>
        </div>
        <!-- /.row -->
        <!-- Page Features -->
        <div class="row">
            {{# each catalog.images }}
                <div class="col-md-4 col-sm-6">
                    <div class="">
                        <img src="/uploads/business/{{this}}" width="250" height="200">
                    </div>
                </div>
            {{/each}}
        </div>
        <hr>
    </div>
  </body>